<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">

 	
 	<bean id="studentBean" class="com.ent.viaConstructor.Student">
 		
		<constructor-arg value="Amit"></constructor-arg>
		<constructor-arg>
			<list>
				<bean class="com.ent.viaConstructor.Subject">
					<constructor-arg index="1" value="Marathi"></constructor-arg>
					<constructor-arg index="0" value="1"></constructor-arg>
				</bean>
				<bean class="com.ent.viaConstructor.Subject">
					<constructor-arg value="2"></constructor-arg>
					<constructor-arg value="English"></constructor-arg>
				</bean>
				<bean class="com.ent.viaConstructor.Subject">
					<constructor-arg value="3"></constructor-arg>
					<constructor-arg value="Maths"></constructor-arg>
				</bean>
			</list>
		</constructor-arg>
 	</bean>      
 	
 	<bean id="shopBean" class="com.ent.viaConstructor.Shop">
 		<constructor-arg value="MTJwellery"></constructor-arg>
 		<constructor-arg>
 			<map>
 				<entry key="pro1" value="4000.00" />
 				<entry key="pro2" value="5000.00" />
 			</map>
 		</constructor-arg>
 	
 	</bean>     
 	
 
 
 
 </beans>